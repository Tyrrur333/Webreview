<%= stylesheet_link_tag "tops", :media => "all" %><main><!--メイントップ-->  <div class="top-wrapper">    <div class="jumbotron jumbotron-extend">      <div class="container">        <div class="jumbotron-container row">          <h1 class="text-center">Webサービスを見つけよう</h1>          <p class="text-center">自分にあったWebサービスを<br>カテゴリやランキング、新着から見つけることができます</p>          <div class="container">            <div class="input-group center-block">              <input type="text" class="form-control" placeholder="サービス名orカテゴリを入力">              <span class="input-group-btn center-block">              <button type="button" class="btn btn-primary"><span>検索</span></button>            </span>            </div>          </div>        </div>      </div>    </div>  </div><!--  Webサービスを探す-->  <div class="container">    <div class="row">      <!--  カテゴリから探す-->      <div class="find-contents col-md-9">        <h2>Webサービスを探す</h2>        <div class="find-category">          <h3>カテゴリから探す</h3>          <div class="category-contents">            <ul>              <li><%= link_to "SNS", "#" %></li>            </ul>          </div>        </div>      </div>      <div class="col-md-9">        <!--  新しく登録されたサイト-->        <div class="new-registration">          <h2>新しく登録されたサイト</h2>        <!--   注目・人気のサイト-->          <div class="attention-posts">            <h2>注目・人気サイト</h2>          </div>        </div>      </div>      <!--  右の最近の投稿-->      <div class="col-md-3">        <div class="right-recent-posts">          <div class="input-group center-block col-md-10">            <input type="text" class="form-control" placeholder="サービス名orカテゴリを入力">            <span class="input-group-btn center-block">              <button type="button" class="btn btn-primary"><span>検索</span></button>            </span>          </div>          <div class="recent-posts">            <h3>最近の投稿</h3>            <div class="recent-contents">              <ul>              </ul>            </div>          </div>        </div>      </div>    </div>  </div></main>